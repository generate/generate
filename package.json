    "get-value": "^2.0.6",
    "global-modules": "^0.2.2",
    "is-answer": "^0.1.0",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "log-utils": "^0.1.4",
    "macro-store": "^0.1.0",
    "mixin-deep": "^1.1.3",
    "parser-front-matter": "^1.3.0",
    "resolve-dir": "^0.1.0",
    "resolve-file": "^0.2.0",
    "set-blocking": "^2.0.0",
    "yargs-parser": "^2.4.0"
  },
  "devDependencies": {
    "base-config-process": "^0.1.7",
    "base-test-runner": "^0.2.0",
    "base-test-suite": "^0.1.11",
    "cross-spawn": "^4.0.0",
    "generate-foo": "^0.1.5",
    "generator-util": "^0.2.9",
    "glob-keys": "^0.1.1",
    "graceful-fs": "^4.1.4",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "is-absolute": "^0.2.5",
    "load-pkg": "^3.0.1",
    "mocha": "^2.5.3",
    "npm-install-global": "^0.1.2",
    "resolve": "^1.1.7",
    "should": "^9.0.2",
    "sinon": "^1.17.4"
  },
  "keywords": [
    "app",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "create",
    "dev",
    "development",
    "end",
    "framework",
    "front",
    "front-end",
    "generate",
    "generator",
    "init",
    "initialize",
    "new",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "stack",
    "template",
    "templates",
    "tool",
    "web",
    "webapp",
    "yeoman",
    "yo"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "postLayout",
      "match": "[*-+]\\s+\\.(\\w)"
    },
    "layout": "minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "description": "Generate shares a common architecture and plugin ecosystem with the following libraries:",
      "list": [
        "assemble",
        "base",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "assemble-core",
      "bach",
      "base",
      "consolidate",
      "generate",
      "generate-dest",
      "gulp",
      "handlebars",
      "lodash",
      "pug",
      "swig",
      "templates",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "lintDeps": {
    "ignore": [
      "code",
      "coverage",
      "docs",
      "examples",
      "lib/tree.js"
    ]
  }
}
