    "assemble-loader": "^0.6.1",
    "base-fs-conflicts": "^0.1.7",
    "base-fs-rename": "^0.1.4",
    "base-generators": "^0.4.3",
    "base-npm": "^0.4.0",
    "base-pipeline": "^0.3.2",
    "base-questions": "^0.6.6",
    "base-runner": "^0.8.2",
    "base-runtimes": "^0.1.11",
    "base-store": "^0.4.4",
    "data-store": "^0.16.0",
    "define-property": "^0.2.5",
    "export-files": "^2.1.1",
    "extend-shallow": "^2.0.1",
